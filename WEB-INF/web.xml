<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
    version="2.4">

  <!-- Order matters in version 2.4 of web.xml! For the elements
       used in this example, this order is required:
          filter
          filter-mapping
          servlet
          servlet-mapping
          welcome-file-list
          error-page
  -->
  

  <servlet>
  	<servlet-name>new-inspection</servlet-name>
  	<servlet-class>screens.NewInspection</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>inspection-result</servlet-name>
  	<servlet-class>screens.InspectionResultServlet</servlet-class>
  </servlet>
	<servlet>
		<servlet-name>company-info</servlet-name>
		<servlet-class>screens.CompanyInfo</servlet-class>
	</servlet>
  <servlet>
  	<servlet-name>pushable-test</servlet-name>
  	<servlet-class>screens.PushableTest</servlet-class>
  </servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>screens.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>inspection</servlet-name>
		<servlet-class>screens.InspectionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>pricing</servlet-name>
		<servlet-class>screens.PricingServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>company-login</servlet-name>
		<servlet-class>screens.CompanyLogin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>control-panel</servlet-name>
		<servlet-class>screens.ControlPanel</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>inspection-info</servlet-name>
		<servlet-class>screens.InspectionInfo</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>edit-technician</servlet-name>
		<servlet-class>screens.EditTechnician</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>edit-field</servlet-name>
		<servlet-class>screens.EditField</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>edit-company</servlet-name>
		<servlet-class>screens.EditCompany</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>edit-area</servlet-name>
		<servlet-class>screens.EditArea</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>edit-lineitem</servlet-name>
		<servlet-class>screens.EditLineItem</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>pricing-upload</servlet-name>
		<servlet-class>screens.PricingFileUpload</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>logo-upload</servlet-name>
		<servlet-class>screens.LogoFileUpload</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>admin</servlet-name>
		<servlet-class>screens.Admin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>edit-company-license</servlet-name>
		<servlet-class>screens.EditCompanyLicense</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>pdf-generator</servlet-name>
		<servlet-class>pdf.PDF_HTML</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>field-info</servlet-name>
		<servlet-class>screens.FieldInfo</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>api</servlet-name>
		<servlet-class>api.Service</servlet-class>
	</servlet>

  <servlet-mapping>
  	<servlet-name>new-inspection</servlet-name>
  	<url-pattern>/new-inspection</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>inspection-result</servlet-name>
  	<url-pattern>/inspection-result</url-pattern>
  </servlet-mapping>
	<servlet-mapping>
		<servlet-name>company-info</servlet-name>
		<url-pattern>/company-info</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
  	<servlet-name>pushable-test</servlet-name>
  	<url-pattern>/pushable-test</url-pattern>
  </servlet-mapping>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>inspection</servlet-name>
		<url-pattern>/inspection</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>pricing</servlet-name>
		<url-pattern>/pricing</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>company-login</servlet-name>
		<url-pattern>/company-login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>control-panel</servlet-name>
		<url-pattern>/control-panel/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>inspection-info</servlet-name>
		<url-pattern>/inspection-info</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>edit-technician</servlet-name>
		<url-pattern>/edit-technician</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>edit-field</servlet-name>
		<url-pattern>/edit-field</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>edit-company</servlet-name>
		<url-pattern>/edit-company</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>edit-area</servlet-name>
		<url-pattern>/edit-area</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>edit-lineitem</servlet-name>
		<url-pattern>/edit-lineitem</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>pricing-upload</servlet-name>
		<url-pattern>/pricing-upload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>logo-upload</servlet-name>
		<url-pattern>/logo-upload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>admin</servlet-name>
		<url-pattern>/admin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>admin</servlet-name>
		<url-pattern>/admin/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>edit-company-license</servlet-name>
		<url-pattern>/edit-company-license</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>pdf-generator</servlet-name>
		<url-pattern>/pdf-generator/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>field-info</servlet-name>
		<url-pattern>/field-info</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>api</servlet-name>
		<url-pattern>/api/*</url-pattern>
	</servlet-mapping>

  
  <welcome-file-list>
    <welcome-file>init.jsp</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  
  <error-page>
    <exception-type>
      javax.servlet.ServletException
    </exception-type>
    <location>/DDE.jsp</location>
  </error-page>
  
  <error-page>
    <error-code>404</error-code>
    <location>/NotFound.jsp</location>
  </error-page>
  
  
</web-app>
